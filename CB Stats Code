<!-- wp:html -->
<!DOCTYPE html>
<html>
<head>
  <title>Group Age</title>
  <style>
    /* CSS to center-align text */
    #result {
      text-align: center;
    }
  </style>
  <script>
    // Function to calculate the difference in years and days between two dates
    function calculateGroupAge() {
      // Date input by the user
      var inputDate = new Date("2020-09-10"); // Set the date to September 10, 2020
      
      // Current date
      var currentDate = new Date();
      
      // Calculate the difference in milliseconds
      var difference = currentDate - inputDate;
      
      // Convert milliseconds to years and remaining days
      var yearsDifference = Math.floor(difference / (1000 * 60 * 60 * 24 * 365.25));
      var remainingDays = Math.floor((difference % (1000 * 60 * 60 * 24 * 365.25)) / (1000 * 60 * 60 * 24));
      
      // Display the result
      document.getElementById("result").innerText = "Group Age: " + yearsDifference + " years, " + remainingDays + " days";
    }

    // Auto-calculate group age when the page finishes loading
    window.onload = function() {
      calculateGroupAge();
    };
  </script>
</head>
<body>

  <!-- Placeholder for the result -->
  <p id="result"></p>

</body>
</html>
<!-- /wp:html -->

